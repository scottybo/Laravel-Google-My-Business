{
    "name": "scottybo/laravel-google-my-business",
    "description": "A package for Laravel which implements the Google My Business API",
    "keywords": ["google", "google my business", "laravel", "laravel5"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Scott Bowler",
            "email": "scott.bowler@dcsworldwide.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "6.* || 7.* || 8.*",
        "google/apiclient": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Scottybo\\LaravelGoogleMyBusiness\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scottybo\\LaravelGoogleMyBusiness\\GoogleMyBusinessServiceProvider"
            ],
            "aliases": {
                "GoogleMyBusiness": "Scottybo\\LaravelGoogleMyBusiness\\Facades\\GoogleMyBusinessFacade"
            }
        }
    }
}
